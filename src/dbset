# sequelize init 또는 db:drop 하고 난 뒤에 해당 source를 실행하면 됩니다.
npx sequelize db:create --env development

# option (옵션)  
npx sequelize model:generate --name Option --attributes extra_price:number,shot_price:number,hot:boolean 

# item (상품)
npx sequelize model:generate --name Item --attributes Option_id:integer,name:string,price:number,type:ENUM,amount:number

# order_item (주문 상품)
npx sequelize model:generate --name Order_item --attributes Item_id:integer,amount:number,state:number

# item_order_customer (주문한 고객)
npx sequelize model:generate --name Item_order_customer --attributes Item_id:integer,Order_customer_id:integer,amount:number

# order_customer (주문한 고객)
npx sequelize model:generate --name Order_customer --attributes state:number